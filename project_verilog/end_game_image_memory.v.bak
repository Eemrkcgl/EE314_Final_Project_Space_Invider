module end_game_image_memory(
	input clk, 
	input [15:0] address,
	output reg [1:0] pixel_data
);

reg end_game_image [36629:0];

initial begin
        $readmemb("./others/end_game_image.txt", end_game_image); 
 end
 
 
  always @(posedge clk) begin
		pixel_data <= end_game_image[address];
    end

endmodule